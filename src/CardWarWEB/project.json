{
	"webroot": "wwwroot",
	"version": "1.0.0-*",

	"dependencies": {
		"Microsoft.AspNet.Hosting": "1.0.0-rc1-final",
		"Microsoft.AspNet.StaticFiles": "1.0.0-rc1-final",
		"Microsoft.AspNet.Server.Kestrel": "1.0.0-rc1-final",
		"Microsoft.AspNet.Http": "1.0.0-rc1-final",
		"Microsoft.VisualStudio.Web.BrowserLink.Loader": "14.0.0-rc1-final",
		"Microsoft.AspNet.Mvc": "6.0.0-rc1-final",
		"Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0-rc1-final",
		"Microsoft.AspNet.Diagnostics": "1.0.0-rc1-final",
		"Microsoft.AspNet.Diagnostics.Entity": "7.0.0-rc1-final",
		"Microsoft.AspNet.Identity": "3.0.0-rc1-final",
		"Newtonsoft.Json": "8.0.3",
		"Microsoft.AspNet.Http.Extensions": "1.0.0-rc1-final",
		"Microsoft.AspNet.WebUtilities": "1.0.0-rc1-final",
		"System.Net.NameResolution": "4.0.0-beta-23019"
	},

	"commands": {
		"kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://0.0.0.0:80"
	},

	"frameworks": {
		"dnxcore50": {
			"compilationOptions": {
				"allowUnsafe": true,
				"debugType": "full"
			},
			"dependencies": {
				"Microsoft.CSharp": "4.0.1-beta-23516",
				"System.Runtime": "4.0.21-beta-23516",
				"System.Security.SecureString": "4.0.0-beta-23019",
				"System.Text.Encoding": "4.0.11-beta-23516",
				"System.IO": "4.0.11-beta-23516",
				"System.Net.WebSockets": "4.0.0-beta-23019",
				"System.Runtime.Numerics": "4.0.1-beta-23516",
				"System.Linq": "4.0.1-beta-23516",
				"System.Threading": "4.0.11-beta-23516",
				"System.Console": "4.0.0-beta-23019",
				"System.Collections": "4.0.11-beta-23516",
				"System.Data.SQLite": "1.0.99",
				"System.Data.SqlClient": "4.0.0-beta-23019",
				"System.Data.Common": "4.0.1-beta-23516",
				"System.Diagnostics.Process": "4.0.0-beta-23019",
				"System.Runtime.Extensions": "4.0.10",
				"System.Net.Requests": "4.0.11-beta-23516",
				"System.Xml.XmlSerializer": "4.0.10-beta-23019",
				"System.Runtime.Serialization.Xml": "4.0.10-beta-23019",
				"System.Runtime.Serialization.Json": "4.0.1-beta-23516",
				"System.Runtime.Serialization.Primitives": "4.0.10-beta-23019"



			}

		}
	},

	"exclude": [
		"wwwroot",
		"node_modules",
		"bower_components"
	],
	"publishExclude": [
		"node_modules",
		"bower_components",
		"**.xproj",
		"**.user",
		"**.vspscc"
	]
}
